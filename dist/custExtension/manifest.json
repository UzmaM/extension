{
    "manifest_version": 2,
    "name": "customExt",
    "description": "custom google extension",
    "version": "1.0",
    "browser_action": {
    "default_icon": "icon2.png",
    "default_popup": "popup.html"
    },
    "permissions": ["storage"],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
   }